  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "lint-staged": {
    "{src,api,cypress}/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --max-warnings=0",
      "git add"
    ],
    "{src,api,cypress}/**/*.{json,css}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 17,
        "branches": 18,
        "functions": 23,
        "lines": 25
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/stories/",
      "/api/",
      "/build/",
      "/cypress/",
      "/docs/",
      "/public/"
    ],
    "setupFiles": [
      "./src/dw/core/helpers/polyfills.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "@gvs(.*)$": "<rootDir>/src/dw/online-configuration/scenes/gvs$1"
    }
  },
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "transform-es2015-modules-commonjs",
        "babel-plugin-dynamic-import-node"
      ]
    }
  },
  "jest-junit": {
    "outputName": "./jest-test-results.xml"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
