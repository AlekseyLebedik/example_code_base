  "scripts": {
    "preinstall": "cd packages/devzone-core && yarn && yarn build && cd ../core-components-webpack-config/ && yarn && yarn build",
    "build:dw": "([ ! -d 'lib' ] && mkdir lib) | true && rm -rf lib/* && cp package.json.dw lib/package.json && cd lib && yarn unlink | true && cd .. && babel --config-file=./.babelrc.dw src/dw -d lib --copy-files --no-copy-ignored --ignore node_modules/**/*.js,src/**/__tests__,src/dw/audit,src/dw/online-configuration,src/dw/reporting && cd lib && yarn link",
    "install:abtesting": "cd guest-apps/abtesting && yarn install",
    "start:abtesting": "cd guest-apps/abtesting && PORT=3001 yarn start",
    "build:abtesting": "cd guest-apps/abtesting && yarn install && yarn build",
    "test:abtesting": "cd guest-apps/abtesting && yarn lint && yarn ci:check-style && yarn test",
    "install:event-manager": "cd guest-apps/event-manager && yarn install",
    "start:event-manager": "cd guest-apps/event-manager && PORT=3003 yarn start",
    "start:event-manager:local-api": "cd guest-apps/event-manager && PORT=3003 yarn start:local-api",
    "build:event-manager": "cd guest-apps/event-manager && yarn install && yarn build",
    "test:event-manager": "cd guest-apps/event-manager && yarn lint && yarn ci:check-style && yarn test",
    "analyze:bundle": "source-map-explorer build/static/js/main.*",
    "analyze:duplication": "bundle-buddy build/static/js/*.map",
    "gen-doc": "esdoc",
    "precommit": "lint-staged && yarn precommit:devzone-core && yarn precommit:abtesting && yarn precommit:event-manager",
    "precommit:devzone-core": "cd packages/devzone-core && yarn precommit",
    "precommit:abtesting": "cd guest-apps/abtesting && yarn precommit",
    "precommit:event-manager": "cd guest-apps/event-manager && yarn precommit",
    "format": "prettier --write '{src,api,cypress}/**/*.{js,jsx,json,css}'",
    "lint": "eslint -c .eslintrc.json 'src/**/*.{js,jsx}' --quiet",
    "lint:fix": "eslint -c .eslintrc.json 'src/**/*.{js,jsx}' --fix",
    "lint:verbose": "eslint -c .eslintrc.json",
    "start": "react-app-rewired --max_old_space_size=4096 start",
    "start:local-api": "env-cmd .env.local-api react-app-rewired --max_old_space_size=4096 start",
    "build": "env-cmd .env.build react-app-rewired build",
    "local:build": "env-cmd .env.build.local react-app-rewired build",
    "test": "yarn && TZ=UTC react-app-rewired test --env=jsdom --testPathIgnorePatterns=src/guest-apps/* --resetMocks=false",
    "test:devzone-core": "cd packages/devzone-core && yarn lint && yarn test",
    "test-CI": "env CI=true yarn test -u",
    "coverage": "yarn test --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook",
    "gen:fake": "node api/faker.js > api/db.json",
    "serve:app": "serve -s -p 3000 build",
    "serve:api": "nodemon --watch api api/index.js",
    "cypress": "cypress open",
    "e2e": "npm-run-all -p serve:api cypress",
    "ci:install": "yarn install --frozen-lockfile --non-interactive",
    "ci:build": "npm-run-all build",
    "ci:serve:app": "serve -s -p 3000 build",
    "cypress:open": "npx cypress open",
    "cypress:run": "cypress run",
    "cypress:install": "cypress install sleep 5",
    "cypress:run:prod": "yarn cypress:run --env configFile=production",
    "cypress:run:local": "yarn cypress:run --env configFile=local",
    "cypress:run:qa": "yarn cypress:run --env configFile=qa --headed",
    "cypress:run:dev": "yarn cypress:run --env configFile=development --headed",
    "cypress:open:prod": "yarn cypress:open --env configFile=production",
    "cypress:open:qa": "yarn cypress:open --env configFile=qa",
    "cypress:open:dev": "yarn cypress:open --env configFile=development",
    "cypress:open:local": "yarn cypress:open --env configFile=local",
    "ci:e2e": "npm-run-all ci:build -r -p ci:serve:* ci:e2e:qa",
    "merge:report": "yarn run mochawesome-merge --reportDir=\"cypress/reports/\"",
    "generate:report": "yarn run marge \"cypress/reports/mochawesome.json\"",
    "after:tests": "npm-run-all merge:report generate:report",
    "ci:e2e:smoke:qa": "yarn cypress:install && yarn cypress:run:qa --spec cypress/integration/smoketests_spec.js",
    "ci:e2e:qa": "yarn cypress:install && yarn cypress:run:qa --config-file cypress/config.json",
    "ci:e2e:prod": "yarn cypress:install && yarn cypress:run:prod --config-file cypress/config.json",
    "ci:e2e:dev": "yarn cypress:install && yarn cypress:run:dev --config-file cypress/config.json",
    "ci:e2e:local": "yarn cypress:install && yarn cypress:run:local --config-file cypress/config.json",
    "ci:check-style": "prettier --list-different '{src,api,cypress}/**/*.{js,jsx,json,css}'",
    "cypress:event-manager": "yarn cypress:run:dev --spec \"cypress/integration/event_manager/**/*\"",
    "ci:e2e:event-manager": "yarn cypress:install && yarn cypress:run:local --spec \"cypress/integration/event_manager/**/*\" --config-file cypress/event-manager-config.json"
  },
